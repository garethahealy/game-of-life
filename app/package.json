{
  "name": "vite-game-of-life",
  "private": true,
  "version": "2.0.1-SNAPSHOT",
  "type": "module",
  "scripts": {
    "dev": "vite --base=/gof",
    "build": "vite build --base=/gof",
    "preview": "vite preview --base=/gof",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,html,json,md,yaml,yml}\" \"public/**/*.{html,css,json}\" \"*.{json,md,yaml,yml}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,html,json,md,yaml,yml}\" \"public/**/*.{html,css,json}\" \"*.{json,md,yaml,yml}\""
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "globals": "17.0.0",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
